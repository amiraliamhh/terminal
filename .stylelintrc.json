{
  "extends": "stylelint-config-standard-scss",
  "overrides": [
    {
      "files": ["**/*.scss"],
      "customSyntax": "postcss-scss"
    }
  ],
  "rules": {
    "number-leading-zero": "never",
    "selector-class-pattern": null,
    "color-function-notation": null
  },
  "ignoreFiles": [
    "coverage/**/*.css"
  ]
}
